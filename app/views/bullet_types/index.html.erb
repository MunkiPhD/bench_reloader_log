<h1>Listing bullet_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Acronym</th>
    <th></th>
  </tr>

<% @bullet_types.each do |bullet_type| %>
  <tr>
    <td><%= bullet_type.name %></td>
    <td><%= bullet_type.acronym %></td>
    <td><%= link_to 'Show', bullet_type %></td>
  </tr>
<% end %>
</table>

<br />

